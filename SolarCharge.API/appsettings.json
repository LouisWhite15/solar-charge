{
  "Inverter": {
    "Type": "Fronius",
    "Url": "http://192.168.1.51"
  },
  "Persistence": {
    "ConnectionString": "Data Source=~/app-persistence/solar-charge.db"
  },
  "Application": {
    "InverterStatusCheckCron": "*/2 * * * *"
  },
  "InfluxDB": {
    "Url": "http://localhost:8086",
    "Token": "AdminToken0=="
  },
  "Features": {
    "IsInfluxDbEnabled": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*"
}
